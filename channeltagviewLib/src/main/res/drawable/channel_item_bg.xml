<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_pressed="true">
        <shape android:shape="rectangle">
            <solid android:color="@color/cmv_white" />
            <corners android:radius="20dp" />
            <stroke android:width="1dp" android:color="@color/cmv_item_tag_stokre_pre" />
        </shape>
    </item>
    <item>
        <shape android:shape="rectangle">
            <solid android:color="@color/cmv_white" />
            <stroke android:width="1dp" android:color="@color/cmv_item_tag_stokre_nor" />
            <corners android:radius="20dp" />
        </shape>
    </item>
</selector>